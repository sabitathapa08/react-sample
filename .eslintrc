{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "max-len": [1, 70, 2, { "ignoreComments": true }],
    "prettier/prettier": ["error"],
    "react/jsx-filename-extension": [
      1,
      { "extensions": [".js", ".jsx"] }
    ],
    "react/prop-types": [2, { "ignore": ["children"] }]
  },
  "env": {
    "browser": true
  }
}
